#include <stdio.h>

#define INF 1000000000
#define MAXN 1005

int map[MAXN][MAXN];
long long dist[MAXN];
int visited[MAXN];
int n, m;

void dijkstra() {
    int i, j, v, u;
    for (i = 1; i <= n; i++) {
        dist[i] = INF;
        visited[i] = 0;
    }
    dist[1] = 0;

    for (i = 1; i <= n; i++) {
        u = -1;
        long long min_d = INF;
        for (j = 1; j <= n; j++) {
            if (!visited[j] && dist[j] < min_d) {
                min_d = dist[j];
                u = j;
            }
        }

        if (u == -1) break;
        visited[u] = 1;

        for (v = 1; v <= n; v++) {
            if (!visited[v] && map[u][v] < INF) {
                if (dist[v] > dist[u] + map[u][v]) {
                    dist[v] = dist[u] + map[u][v];
                }
            }
        }
    }
}

int main() {
    int i, j, u, v, q;
    if (scanf("%d %d", &n, &m) != 2) return 0;

    for (i = 1; i <= n; i++) {
        for (j = 1; j <= n; j++) {
            if (i == j) map[i][j] = 0;
            else map[i][j] = INF;
        }
    }

    for (i = 0; i < m; i++) {
        if (scanf("%d %d %d", &u, &v, &q) == 3) {
            if (u >= 1 && u <= n && v >= 1 && v <= n) {
                if (q < map[u][v]) {
                    map[u][v] = q;
                    map[v][u] = q;
                }
            }
        }
    }

    dijkstra();

    if (dist[n] >= INF) {
        printf("so luong qua it nhat:-1");
    } else {
        printf("so luong qua it nhat:%lld", dist[n]);
    }

    return 0;
}
