#include <stdio.h>

#define INF 1000000000

typedef struct {
    int r, c, cost;
} Node;

Node heap[1000005];
int heapSize = 0;

void swap(Node *a, Node *b) {
    Node temp = *a;
    *a = *b;
    *b = temp;
}

void push(int r, int c, int cost) {
    heapSize++;
    heap[heapSize].r = r;
    heap[heapSize].c = c;
    heap[heapSize].cost = cost;
    int i = heapSize;
    while (i > 1 && heap[i].cost < heap[i / 2].cost) {
        swap(&heap[i], &heap[i / 2]);
        i /= 2;
    }
}

Node pop() {
    Node top = heap[1];
    heap[1] = heap[heapSize];
    heapSize--;
    int i = 1;
    while (i * 2 <= heapSize) {
        int child = i * 2;
        if (child + 1 <= heapSize && heap[child + 1].cost < heap[child].cost) {
            child++;
        }
        if (heap[i].cost <= heap[child].cost) break;
        swap(&heap[i], &heap[child]);
        i = child;
    }
    return top;
}

int a[505][505];
int d[505][505];

int main() {
    int M, N;
    if (scanf("%d %d", &M, &N) != 2) return 0;

    for (int i = 0; i < M; i++) {
        for (int j = 0; j < N; j++) {
            scanf("%d", &a[i][j]);
            d[i][j] = INF;
        }
    }

    int dr[] = {-1, 1, 0, 0};
    int dc[] = {0, 0, -1, 1};

    d[0][0] = a[0][0];
    push(0, 0, d[0][0]);

    while (heapSize > 0) {
        Node curr = pop();

        if (curr.cost > d[curr.r][curr.c]) continue;
        if (curr.r == M - 1 && curr.c == N - 1) break;

        for (int i = 0; i < 4; i++) {
            int nr = curr.r + dr[i];
            int nc = curr.c + dc[i];

            if (nr >= 0 && nr < M && nc >= 0 && nc < N) {
                if (d[curr.r][curr.c] + a[nr][nc] < d[nr][nc]) {
                    d[nr][nc] = d[curr.r][curr.c] + a[nr][nc];
                    push(nr, nc, d[nr][nc]);
                }
            }
        }
    }

    printf("%d", d[M - 1][N - 1]);
    return 0;
}
