#include <stdio.h>

#define MAX_N 100005
#define MAX_M 100005

// Sử dụng danh sách kề để lưu trữ đồ thị hiệu quả hơn ma trận kề
int head[MAX_N];
int next_edge[MAX_M];
int to[MAX_M];
int edge_count = 0;

void add_edge(int u, int v) {
    to[edge_count] = v;
    next_edge[edge_count] = head[u];
    head[u] = edge_count;
    edge_count++;
}

int main() {
    int n, m;
    // Đọc số bé (n) và số ràng buộc (m)
    if (scanf("%d %d", &n, &m) != 2) return 0;

    // Khởi tạo head của danh sách kề
    for (int i = 1; i <= n; i++) {
        head[i] = -1;
    }
    edge_count = 0;

    int in_degree[MAX_N] = {0}; // Bậc vào của mỗi đỉnh
    
    // Đọc m ràng buộc và xây dựng đồ thị
    for (int i = 0; i < m; i++) {
        int a, b;
        scanf("%d %d", &a, &b);
        // Bé 'a' muốn nhiều hơn 'b' => ràng buộc đi từ b đến a
        add_edge(b, a);
        in_degree[a]++;
    }

    int candies[MAX_N];
    for (int i = 1; i <= n; i++) {
        candies[i] = 1; // Mặc định mỗi bé có ít nhất 1 viên kẹo
    }

    // --- Thuật toán Sắp xếp Topo dùng Queue ---
    int queue[MAX_N];
    int q_front = 0, q_rear = 0;

    // Đưa tất cả các bé không có ai muốn kẹo nhiều hơn mình vào hàng đợi
    for (int i = 1; i <= n; i++) {
        if (in_degree[i] == 0) {
            queue[q_rear++] = i;
        }
    }

    long long totalCandies = 0;

    while (q_front < q_rear) {
        int u = queue[q_front++];
        
        // Duyệt qua các bé v mà u là tiền nhiệm
        for (int i = head[u]; i != -1; i = next_edge[i]) {
            int v = to[i];
            
            // Cập nhật số kẹo cho bé v: phải lớn hơn bé u ít nhất 1
            if (candies[v] < candies[u] + 1) {
                candies[v] = candies[u] + 1;
            }
            
            in_degree[v]--;
            // Nếu v không còn tiền nhiệm, đưa vào hàng đợi
            if (in_degree[v] == 0) {
                queue[q_rear++] = v;
            }
        }
    }
    // --- Kết thúc Sắp xếp Topo ---

    // In số kẹo của từng bé và tính tổng
    for (int i = 1; i <= n; i++) {
        printf("%d\n", candies[i]);
        totalCandies += candies[i];
    }

    // In tổng số kẹo ít nhất cần chuẩn bị
    printf("%lld\n", totalCandies);

    return 0;
}
