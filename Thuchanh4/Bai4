#include <stdio.h>

#define MAX_N 100005
#define MAX_M 100005

// Sử dụng danh sách kề để lưu trữ đồ thị
int head[MAX_N];
int next_edge[MAX_M];
int to[MAX_M];
int edge_count = 0;

void add_edge(int u, int v) {
    to[edge_count] = v;
    next_edge[edge_count] = head[u];
    head[u] = edge_count;
    edge_count++;
}

int main() {
    int n, m;
    // Đọc số đỉnh (n) và số cung (m)
    if (scanf("%d %d", &n, &m) != 2) return 0;

    // Khởi tạo head của danh sách kề
    for (int i = 1; i <= n; i++) {
        head[i] = -1;
    }
    edge_count = 0;

    int in_degree[MAX_N] = {0}; // Bậc vào của mỗi đỉnh
    
    // Đọc m cung và xây dựng đồ thị
    for (int i = 0; i < m; i++) {
        int u, v;
        scanf("%d %d", &u, &v);
        add_edge(u, v); // Cung từ u đến v
        in_degree[v]++;
    }

    int rank[MAX_N];
    for (int i = 1; i <= n; i++) {
        rank[i] = 0; // Hạng ban đầu của mọi đỉnh là 0
    }

    // --- Thuật toán Sắp xếp Topo dùng Queue ---
    int queue[MAX_N];
    int q_front = 0, q_rear = 0;

    // Đưa tất cả các đỉnh nguồn (bậc vào = 0) vào hàng đợi
    for (int i = 1; i <= n; i++) {
        if (in_degree[i] == 0) {
            queue[q_rear++] = i;
        }
    }

    while (q_front < q_rear) {
        int u = queue[q_front++];
        
        // Duyệt qua các đỉnh v kề với u
        for (int i = head[u]; i != -1; i = next_edge[i]) {
            int v = to[i];
            
            // Cập nhật hạng cho đỉnh v: max(rank[v], rank[u] + 1)
            if (rank[v] < rank[u] + 1) {
                rank[v] = rank[u] + 1;
            }
            
            in_degree[v]--;
            // Nếu v không còn tiền nhiệm, đưa vào hàng đợi
            if (in_degree[v] == 0) {
                queue[q_rear++] = v;
            }
        }
    }
    // --- Kết thúc Sắp xếp Topo ---

    // In hạng của từng đỉnh theo thứ tự đỉnh từ 1 đến n
    for (int i = 1; i <= n; i++) {
        printf("%d\n", rank[i]);
    }

    return 0;
}
