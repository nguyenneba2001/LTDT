#include <stdio.h>

int a[1005][1005], d[1005], q[1005], res[1005], n, m;

int main() {
    int i, j, u, v, h = 0, t = 0, c = 0;
    if (scanf("%d %d", &n, &m) != 2) return 0;

    for (i = 1; i <= n; i++) {
        d[i] = 0;
        for (j = 1; j <= n; j++) a[i][j] = 0;
    }

    for (i = 0; i < m; i++) {
        if (scanf("%d %d", &u, &v) == 2) {
            if (a[u][v] == 0) {
                a[u][v] = 1;
                d[v] = d[v] + 1;
            }
        }
    }

    for (i = 1; i <= n; i++) {
        if (d[i] == 0) q[t++] = i;
    }

    while (h < t) {
        u = q[h++];
        res[c++] = u;
        for (v = 1; v <= n; v++) {
            if (a[u][v] == 1) {
                d[v] = d[v] - 1;
                if (d[v] == 0) q[t++] = v;
            }
        }
    }

    for (i = 0; i < c; i++) {
        printf("%d", res[i]);
        if (i < c - 1) printf(" ");
    }
    printf("\n");
    return 0;
}
