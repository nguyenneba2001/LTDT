#include <stdio.h>

#define MAXN 105
#define MAXM 505

int n, m;
int u[MAXM], v[MAXM];
int indeg[MAXN];
int rankv[MAXN];
int queue[MAXN];

int main() {
    int i, e;
    int front, rear;
    int x, y;

    scanf("%d%d", &n, &m);

    for (i = 1; i <= n; i++) {
        indeg[i] = 0;
        rankv[i] = 0;
    }

    for (e = 0; e < m; e++) {
        scanf("%d%d", &u[e], &v[e]);
        indeg[v[e]]++;
    }

    /* Khởi tạo queue với các đỉnh indeg = 0 */
    front = 0;
    rear = 0;
    for (i = 1; i <= n; i++) {
        if (indeg[i] == 0) {
            queue[rear++] = i;
            rankv[i] = 0;
        }
    }

    /* Topological + cập nhật hạng */
    while (front < rear) {
        x = queue[front++];

        for (e = 0; e < m; e++) {
            if (u[e] == x) {
                y = v[e];
                if (rankv[y] < rankv[x] + 1)
                    rankv[y] = rankv[x] + 1;

                indeg[y]--;
                if (indeg[y] == 0)
                    queue[rear++] = y;
            }
        }
    }

    /* In kết quả */
    for (i = 1; i <= n; i++) {
        printf("%d\n", rankv[i]);
    }

    return 0;
}
